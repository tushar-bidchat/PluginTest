<?xml version='1.0' encoding='utf-8'?>
<widget id="com.bidchat.plugintest" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PluginTest</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="BackupWebStorage" value="none" />
    <preference name="analytics_write_key" value="eBqPrkMxNcBIb8Sw6ne98rpsiVYWBCAL" />
    <preference name="analytics_debug_write_key" value="eBqPrkMxNcBIb8Sw6ne98rpsiVYWBCAL" />
    <branch-config>
        <ios-team-id value="3R8NJ8ABKK" />
        <android-prefix value="/X9Ug" />
        <host name="bnc.lt" scheme="https" />
    </branch-config>
    <plugin name="cordova-plugin-facebook4" spec="~1.7.1">
        <variable name="APP_ID" value="1643392872598880" />
        <variable name="APP_NAME" value="Bidchat" />
    </plugin>
</widget>
